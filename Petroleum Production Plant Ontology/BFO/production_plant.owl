                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Offshore_Petroleum_Production_Plant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:source xml:lang="en">https://www.glossary.oilfield.slb.com/en/Terms/m/manifold.aspx</dc:source>
        <CommonCoreOntologies:definition xml:lang="en">def. equipment that is controls, distributes and typically monitors fluid flow.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">An arrangement of piping or valves designed to control, distribute and typically monitor fluid flow.</rdfs:comment>
        <rdfs:label xml:lang="en">Manifold</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#NaturalGas -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#NaturalGas">
