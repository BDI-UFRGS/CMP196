        <owl:disjointWith rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Onshore_Petroleum_Production_Plant"/>
        <CommonCoreOntologies:definition xml:lang="en">def. A petroleum production plant that is offshore.</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">OffshorePetroleumProductionPlant</rdfs:label>
    </owl:Class>
    


