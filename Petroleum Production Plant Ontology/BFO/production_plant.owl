        <CommonCoreOntologies:definition>A type of fluid pump in which the displacement volume of the pump is fixed for each rotation of the pump.</CommonCoreOntologies:definition>
        <rdfs:label>Positive-DisplacementPump</rdfs:label>
        <rdfs:seeAlso>https://www.glossary.oilfield.slb.com/Terms/p/positive-displacement_pump.aspx</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#ProducedWater -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#ProducedWater">
