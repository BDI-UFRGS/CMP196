        <CommonCoreOntologies:definition xml:lang="en">A petroleum production plant that is onshore.</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">OnshorePetroleumProductionPlant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#PLEM -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#PLEM">
