            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002350"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Petroleum_Production_Plant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#separates"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Fluid"/>
