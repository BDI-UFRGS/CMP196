    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#BottomholePressure -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#BottomholePressure">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000052"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CompletedWell"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Pressure"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition xml:lang="en">def. the Pressure, usually measured in pounds per square inch (psi), at the bottom of the hole.</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">BottomholePressure</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.glossary.oilfield.slb.com/Terms/b/bottomhole_pressure.aspx</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Capacity -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Capacity">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000019"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000080"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Tank"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Volume"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition xml:lang="en">def. the maximum amount that something can contain.</CommonCoreOntologies:definition>
        <rdfs:label xml:lang="en">Capacity</rdfs:label>
        <rdfs:seeAlso xml:lang="en">https://www.lexico.com/en/definition/capacity</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CentrifugalPump -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CentrifugalPump">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Pump"/>
        <owl:disjointWith rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Positive-DisplacementPump"/>
        <CommonCoreOntologies:definition>def. a Pump where the rotary motion of a profiled impeller in combination with a shaped pump housing or volute applies centrifugal force to discharge fluids from the pump.</CommonCoreOntologies:definition>
        <rdfs:label>CentrifugalPump</rdfs:label>
        <rdfs:seeAlso>https://www.glossary.oilfield.slb.com/Terms/c/centrifugal_pump.aspx</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#ChristmasTree -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#ChristmasTree">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/core/iof_00000045"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002350"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Petroleum_Production_Plant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:hasRelatedSynonym xml:lang="en">Tree</oboInOwl:hasRelatedSynonym>
        <oboInOwl:source rdf:resource="https://en.wikipedia.org/wiki/Christmas_tree_(oil_well)"/>
        <CommonCoreOntologies:definition xml:lang="en">def. equipment that is used to regulate the flow of pipes in an completed wells.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">In petroleum and natural gas extraction, a Christmas tree, or &quot;tree&quot;, is an assembly of valves, spools, and fittings used to regulate the flow of pipes in an oil well, gas well, water injection well, water disposal well, gas injection well, condensate well and other types of wells.</rdfs:comment>
        <rdfs:label xml:lang="en">Christmas Tree</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CompletedWell -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CompletedWell">
        <rdfs:subClassOf rdf:resource="https://www.industrialontologies.org/core/iof_00000045"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0001025"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Well"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002350"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Petroleum_Production_Plant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:source xml:lang="en">https://petrowiki.spe.org/Glossary:Completed_well</oboInOwl:source>
        <CommonCoreOntologies:definition xml:lang="en">def. equipment that is used to produce hydrocarbons or inject fluids.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">A well that has been drilled, cased and cemented and is ready to produce hydrocarbons.</rdfs:comment>
        <rdfs:label xml:lang="en">CompletedWell</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CrudeOil -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CrudeOil">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Fluid"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/BFO_0000050"/>
                <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Petroleum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <CommonCoreOntologies:definition>def. a mixture of hydrocarbons that exists as a liquid in underground geologic formations and remains a liquid when brought to the surface.</CommonCoreOntologies:definition>
        <rdfs:label>CrudeOil</rdfs:label>
        <rdfs:seeAlso>https://en.wikipedia.org/wiki/Petroleum</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#FPSO -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#FPSO">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Offshore_Platform"/>
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Ship"/>
        <owl:disjointWith rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Semisubmersible"/>
        <oboInOwl:source xml:lang="en">https://www.glossary.oilfield.slb.com/Terms/f/fpso.aspx</oboInOwl:source>
        <CommonCoreOntologies:acronym xml:lang="en">FPSO</CommonCoreOntologies:acronym>
        <CommonCoreOntologies:definition xml:lang="en">def. an offshore platform that serve as gathering, processing and storage facilities for fluids produced from subsea wells.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">Abbreviation for floating production, storage, and offloading vessel. FPSOs are commonly deployed for offshore production in locations that have little infrastructure. They serve as gathering, processing and storage facilities for fluids produced from subsea wells. Processed using equipment on the ship’s deck, the fluids are then stored in the ship’s hull before they are offloaded to pipelines, barges, or ships.</rdfs:comment>
        <rdfs:label xml:lang="en">FloatingProductionStorageOffloading</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Flare -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Flare">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000027"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#burns"/>
                <owl:allValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#NaturalGas"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:source xml:lang="en">https://www.glossary.oilfield.slb.com/Terms/f/flare.aspx</oboInOwl:source>
        <CommonCoreOntologies:definition xml:lang="en">def. equipment that is used to burn combustible vapors.</CommonCoreOntologies:definition>
        <rdfs:comment xml:lang="en">An arrangement consisting of a vertical tower and burners used to burn combustible vapors. A flare is usually situated near a producing well or at a gas plant or refinery. A flare is also called a flare stack.</rdfs:comment>
        <rdfs:label xml:lang="en">Flare</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Fluid -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Fluid">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000024"/>
        <CommonCoreOntologies:definition>def. a substance that continually deforms (flows) under an applied shear stress, or external force. </CommonCoreOntologies:definition>
        <rdfs:label>Fluid</rdfs:label>
        <rdfs:seeAlso>https://en.wikipedia.org/wiki/Fluid</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#InjectionWell -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#InjectionWell">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#CompletedWell"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#injects"/>
                        <owl:someValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Fluid"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0000085"/>
                        <owl:allValuesFrom rdf:resource="http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#toInject"/>
                    </owl:Restriction>
                    <owl:Restriction>
