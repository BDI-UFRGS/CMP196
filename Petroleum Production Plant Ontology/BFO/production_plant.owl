        <rdfs:comment xml:lang="en">An offshore structure from which a well may be drilled or produced.</rdfs:comment>
        <rdfs:label xml:lang="en">OffshorePlatform</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/~nosantos/ontologias/production_plant#Onshore_Petroleum_Production_Plant -->

